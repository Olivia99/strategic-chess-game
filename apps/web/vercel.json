{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/build/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/client/$1"
    }
  ],
  "outputDirectory": "build/client"
}