{
  "buildCommand": "npm run build",
  "outputDirectory": "build/client",
  "framework": null,
  "functions": {
    "build/server/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/build/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/build/client/$1"
    }
  ]
}